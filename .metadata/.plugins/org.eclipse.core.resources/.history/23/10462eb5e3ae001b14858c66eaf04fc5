package org.soft.main;

import org.apache.logging.log4j.LogManager;
import org.apache.logging.log4j.Logger;
import org.soft.service.StudentManager;
import org.soft.vo.Student;
import org.apache.logging.log4j.LogManager;
import org.apache.logging.log4j.Logger;

public class SMTest {
	public static void main(String[] args) {	
		private static Logger logger = LogManager.getLogger(StudentManager.class.getName());
		// 问题编号：27,48
		final int max = 20;
		Student stu[] = new Student[max]; // 创建学生类型数组
		int tab[] = new int[max]; // 建立一个数组用于标记学生数组中的存储情况
		StudentManager sm = new StudentManager(); 
		sm.StudentManageApp(stu,tab); // 调用app方法控制程序流程
	}
}
